    "quote_request": {
      "name": "Quote Request Form",
      "description": "Primary lead capture form for quote requests",
      "redirect_url": "/thank-you/quote",
      "tags": ["quote_request", "lead"],
      "pipeline": "New Leads",
      "stage": "New Lead",
      "visible_fields": [
        {
          "name": "first_name",
          "label": "First Name",
          "type": "text",
          "required": true,
          "ghl_field": "contact.first_name"
        },
        {
          "name": "last_name",
          "label": "Last Name",
          "type": "text",
          "required": true,
