          "name": "email",
          "label": "Email",
          "type": "email",
          "required": true,
          "ghl_field": "contact.email"
        },
        {
          "name": "phone",
          "label": "Phone",
          "type": "tel",
          "required": true,
          "ghl_field": "contact.phone"
        },
        {
          "name": "company",
          "label": "Company Name",
          "type": "text",
          "required": false,
          "ghl_field": "contact.company_name"
        },
        {
          "name": "service_interest",
          "label": "What do you need?",
          "type": "select",
          "required": true,
          "ghl_field": "contact.service_interest",
          "options": [
            {"value": "", "label": "Select a service..."},
            {"value": "pallet_racking", "label": "Pallet Racking (New)"},
            {"value": "used_rack", "label": "Used Pallet Rack"},
            {"value": "warehouse_design", "label": "Warehouse Design & Layout"},
            {"value": "shelving", "label": "Shelving Systems"},
            {"value": "installation", "label": "Installation / Teardown"},
            {"value": "relocation", "label": "Warehouse Relocation"},
            {"value": "permitting", "label": "Permitting & Engineering"},
            {"value": "safety_inspection", "label": "Rack Safety Inspection"},
            {"value": "other", "label": "Other / General Inquiry"}
          ]
        },
        {
          "name": "project_details",
          "label": "Project Details",
          "type": "textarea",
          "required": false,
          "placeholder": "Tell us about your project: warehouse size, number of pallet positions needed, timeline, etc.",
          "ghl_field": "contact.notes"
        },
        {
          "name": "timeline",
          "label": "Project Timeline",
          "type": "select",
          "required": false,
          "ghl_field": "contact.timeline",
          "options": [
            {"value": "", "label": "Select timeline..."},
            {"value": "asap", "label": "ASAP / Urgent"},
            {"value": "1_month", "label": "Within 1 month"},
            {"value": "1_3_months", "label": "1-3 months"},
            {"value": "3_6_months", "label": "3-6 months"},
            {"value": "planning", "label": "Just planning / researching"}
          ]
        }
      ],
